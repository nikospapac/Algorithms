void RemoveDuplicates( struct node* head ) {
	struct node* next, current = head;
	int prevnum;

	if ( current == NULL ) {
		return;
	}
	while ( current->next != NULL ) {
		if ( current->data == current->next->data ) {
			next = current->next->next;
			free(current->next);
			current->next = next;
		}
		else {
			current = current->next;
		}
	}
}
