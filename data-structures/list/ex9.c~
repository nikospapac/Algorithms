void FrontBackSplit ( struct node* source, struct node** frontRef, struct node** backRef ){
	struct node* current = source;
	int i;
	int len = 0;
	len = Lenght(current);
	if ( len < 2 ) {
		*frontRef = source;
		*backRef = NULL;
	}
	else {
		if ( len % 2 != 0 ) {
			len = ( len / 2 ) + 1;
		}
		else { 
			len = len / 2;
		}
		for ( i = 0; i < len; ++i ) {
			current = current->next;
		}
		*frontRef = source;
		*backRef = current->next;

		current->next = NULL;
	}
}
